# Environment Variables Configuration
# Copy this file to .env and replace the placeholder values with your actual API keys

# Google AI (Gemini) API Key
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# AssemblyAI API Key (for speech-to-text)
# Get your API key from: https://www.assemblyai.com/
ASSEMBLY_AI_API_KEY=your_assemblyai_api_key_here

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True

# Other API Keys (add as needed)
# Add any additional API keys your application requires here
